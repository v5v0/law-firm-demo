{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#0f232e",
					"name": "Primary Dark"
				},
				{
					"slug": "secondary",
					"color": "#c79c60",
					"name": "Secondary Gold"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "light-gray",
					"color": "#f5f5f5",
					"name": "Light Gray"
				},
				{
					"slug": "dark-gray",
					"color": "#333333",
					"name": "Dark Gray"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "ui-serif, Georgia, Cambria, 'Times New Roman', Times, serif",
					"slug": "serif",
					"name": "Serif"
				},
				{
					"fontFamily": "ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
					"slug": "sans",
					"name": "Sans Serif"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2.25rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "3rem",
					"name": "XX Large"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "0"
				}
			}
		}
	}
}

